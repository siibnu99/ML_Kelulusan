   "source": [
    "from flask import Flask, request\n",
    "\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route('/process_post_data', methods=['POST'])\n",
    "def process_post_data():\n",
    "    data = request.form['input_data']\n",
    "    return f'Received data: {data}'\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    app.run(debug=True)"
   ]
  }
